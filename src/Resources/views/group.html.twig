<li class="">
    <a href="javascript:;">
        <i class="{{ group.icon }}"></i>
        <span class="title"> {{ group.name }}</span>
        <span class="selected"></span>
        {% if group.childs|length >0 %}
        <span class="arrow"></span>
        {% endif %}
    </a>
    {% if group.childs|length >0 %}
    <ul class="sub-menu">
        {% for k, menu in group.childs %}
        {{ include('@Menu/menu.html.twig', {menu: menu}) }}
        {% endfor %}
    </ul>
    {% endif %}
</li>